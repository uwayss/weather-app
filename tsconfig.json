{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
